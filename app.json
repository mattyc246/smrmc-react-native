{
  "expo": {
    "name": "SMRMC",
    "slug": "smrmc",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#32393d"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.matthewcross.smrmc"
    },
    "description": "Self Management App - To Do List Manager",
    "githubUrl": "https://github.com/mattyc246/smrmc-react-native"
  }
}